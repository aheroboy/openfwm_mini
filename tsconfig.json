{
  "compilerOptions": {
    "noEmit": true,
    "strictNullChecks": true,
    "noImplicitAny": false,
    "module": "CommonJS",
    "target": "ES2020",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "alwaysStrict": false,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": false,
    "strictPropertyInitialization": false,
    "lib": ["ES2020"],
    "typeRoots": [
      "types"
    ]
  },
  "include": [
    "./**/*.ts", "miniprogram/pages/spots/index.ts" 
, "miniprogram/utils/math.ts"  ],
  "exclude": [
    "node_modules"
  ],
}
